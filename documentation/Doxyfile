EXCLUDE_SYMLINKS       = YES
SOURCE_BROWSER         = YES
INLINE_SOURCES         = YES
STRIP_CODE_COMMENTS    = NO
SOURCE_TOOLTIPS        = YES
GENERATE_LATEX         = YES
